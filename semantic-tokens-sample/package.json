{
	"name": "jinja-sql-syntax-highlighter",
	"displayName": "jinja-sql-syntax-highlighter",
	"description": "Highlights jinja syntax in sql files",
	"version": "0.0.1",
	"publisher": "BryanR",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/BryanRacic/jinja-sql-syntax-vscode"
	},
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:sql"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "sql",
				"extensions": [
					".sql"
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.11.7",
		"@types/vscode": "^1.42.0",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"eslint": "^8.13.0",
		"typescript": "^4.7.2"
	}
}
