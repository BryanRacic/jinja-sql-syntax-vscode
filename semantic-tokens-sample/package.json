{
	"name": "jinja-sql-syntax-highlighter",
	"displayName": "jinja-sql-syntax-highlighter",
	"description": "Highlights jinja syntax in sql files",
	"version": "0.0.2",
	"publisher": "BryanR",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/BryanRacic/jinja-sql-syntax-vscode"
	},
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
		  {
			"id": "sql",
			"extensions": [
			  ".sql",
			  ".dsql"
			],
			"aliases": [
			  "SQL"
			],
			"configuration": "./language-configuration.json"
		  }
		],
	"grammars": [
		{
		  "language": "sql",
		  "scopeName": "source.sql",
		  "path": "./syntaxes/sql.tmLanguage.json"
		}
	  ]
	}
}
